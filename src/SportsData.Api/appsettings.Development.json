    "ApiUrl": "http://localhost:5068/api/"
  },
  "PlayerProviderConfig": {
    "SecretKey": "foo",
    "ApiUrl": "http://localhost:5163/api/"
  },
  "ProducerProviderConfig": {
    "SecretKey": "foo",
    "ApiUrl": "http://localhost:5231/api/"
